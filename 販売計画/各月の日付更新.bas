Sub 更新日_01月()
    Dim rc As VbMsgBoxResult
    rc = MsgBox("該当の月の更新日をD列へ記録しますか？", vbYesNo + vbQuestion)
    If rc = vbNo Then
        MsgBox "中止します", vbCritical
        Exit Sub
    End If
    Cells(5 + 1, 4) = Now  '各月の更新日を入力
End Sub
